{
  "name": "Dummy PWA",
  "short_name": "Dummy",
  "id": "dummy500",
  "start_url": "/",
  "display": "fullscreen",
  "background_color": "#ffffff",
  "theme_color": "#4CAF50",
  "orientation": "any",
  "icons": [
    {
      "src": "images/icon.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "description": "A dummy PWA.",
  "screenshots": [
    {
      "src": "images/screenshot.png",
      "sizes": "1419x924",
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "Open Sync",
      "url": "/index.html#sync-data",
      "icons": [
        {
          "src": "/images/icon.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [
          ".pdf"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+books",
      "url": "/?book=%s"
    }
  ],
  "display_override": [
    "window-controls-overlay"
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "edge_side_panel": {
    "preferred_width": 15
  },
  "widgets": [
    {
      "name": "Dummy PWA",
      "description": "widget to control the dummy pwa app",
      "tag": "dpwa",
      "template": "dpwa-template",
      "ms_ac_template": "dummy-widgets.json",
      "data": "dummy-widgets.json",
      "type": "application/json",
      "screenshots": [
        {
          "src": "./images/screenshot.png",
          "sizes": "600x400",
          "label": "The PWAmp mini-player widget"
        }
      ],
      "icons": [
        {
          "src": "./images/icon.png",
          "sizes": "16x16"
        }
      ],
      "auth": false,
      "update": 86400
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "lang": "en",
  "dir": "ltr",
  "categories": [
    "books",
    "education",
    "entertainment"
  ],
  "iarc_rating_id": "e58c174a-81d2-5c3c-32cc-34b8de4a52e9",
  "prefer_related_applications": true,
  "related_applications": [
    {
      "platform": "windows",
      "url": "https://dummy-pwa-orcin.vercel.app/",
      "id": "dummypwa.DummyApp"
    }
  ],
  "scope": "https://dummy-pwa-orcin.vercel.app/",
  "scope_extensions": [
    {
      "origin": "*.dummy-pwa-orcin.vercel.app"
    }
  ]
}