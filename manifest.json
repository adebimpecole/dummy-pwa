{
  "orientation": "any",
  "name": "Dummy PWA",
  "short_name": "Dummy",
  "start_url": "/",
  "display": "fullscreen",
  "background_color": "#ffffff",
  "theme_color": "#4CAF50",
  "icons": [
    {
      "src": "images/icon.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "description": "A dummy PWA.",
  "screenshots": [
    {
      "src": "images/screenshot.png",
      "sizes": "1419x924",
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "Open Sync",
      "url": "/index.html#sync-data",
      "icons": [
        {
          "src": "/images/icon.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [
          ".pdf"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+books",
      "url": "/?book=%s"
    }
  ],
  "display_override": [
    "window-controls-overlay"
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "edge_side_panel": {
    "preferred_width": 15
  },
  "lang": "en",
  "dir": "ltr"
}